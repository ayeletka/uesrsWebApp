@model myWebApp.Models.Users
@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<form action="/Home/ShowUsers" method="get">
    <input type="button" value="Show all users" onclick="location.href='@Url.Action("ShowUsers", "Home")'" />

</form>

@foreach (var element in Model.getUsersList())
{
    <p>@Html.DisplayFor(m => element)</p>
}